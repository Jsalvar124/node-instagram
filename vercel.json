{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      },
      {
        "src": "/api/cron",
        "dest": "/api/cronjob"
      }
    ],
    "crons": [
        {
          "schedule": "0 21 * * *",
          "path": "/api/cronjob"
        }
      ]
}

  
  