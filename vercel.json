{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "crons": [
        {
          "path": "/api/cron",
          "schedule": "0 17 * * *"
        }
      ]
}