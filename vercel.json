{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      },
      {
        "src": "/api/cron",
        "dest": "/api/cronjob"
      }
    ],
    "functions": {
        "cronjob": {
          "includeFiles": "cronjob.js"
        }
      },
    "crons": [
        {
          "function": "cronjob",
          "schedule": "0 20 * * *",
          "path": "/api/cronjob"
        }
      ]
}